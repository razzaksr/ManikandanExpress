<a href="/techie" class="btn btn-primary"><i class="fa fa-plus"></i>Add New</a>
<table class="table table-striped table-responsive-lg p-5 rounded shadow table-hover">
    <thead class="thead-dark">
        <tr>
            <th>Full Name</th><th>Contact Number</th>
            <th>Experience</th><th>Technology</th>
            <th>Commercial</th>
        </tr>
    </thead>
    <tbody>
        {{#each all}}
            <tr>
                <td>
                    <span>
                        <a href="/techie/delete/{{this._id}}">
                            <i class="fa fa-trash"></i>
                        </a>
                    </span>
                    {{this.fullname}}
                </td>
                
                <td>
                    <span>
                        <a href="/techie/edit/{{this._id}}">
                            <i class="fa fa-pencil"></i>
                        </a>
                    </span>
                    {{this.contact}}
                </td>
                
                <td>{{this.experience}}</td>
                <td>{{this.expert}}</td>
                <td>{{this.commercials}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>